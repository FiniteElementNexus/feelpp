// -*- mode: javascript -*-
{
    "Name": "Turek",
    "Model":"Navier-Stokes",
    "Description":"Fluid flow around a cylinder",
    "Parameters":
    {
        "Te": 293.1,
        "Um":
        {
            "type":"value",
            "value":0.3,
            "min":1e-4,
            "max":10,
            "description":"Um desc"
        },
        "H":
        {
            "type":"expression",
            "value":0.41
        }
    },
    "Materials":
    {
        "myfluid":
        {
            "markers":["fluid1","fluid2"],
            "physics": "electro",
            "rho": "2", // density
            "eta": "0.001", // dynamic_viscosity
            "mu": "{1,t}:t",
            "nu": "{1,2,x*y}:x:y:z",
            "chi": "{1,2,x,y}:x:y",
            "xhi": "{1,2,3,4,5,6,7,8,t}:t",
            "f" : "2*g:g",
            "h" : "2*g+t:g:t"
        },
	    "mycopper":
        {
            "markers":"copper",
            "physics":["electro","thermo"],
            "rho": "1", // density
            "eta": "0.001", // dynamic_viscosity
            "mu": "{1,t}:t",
            "nu": "{1,2,x*y}:x:y:z",
            "chi": "{1,2,x,y}:x:y",
            "xhi": "{1,2,3,4,5,6,7,8,t}:t",
            "f" : "2*g:g",
            "h" : "2*g+t:g:t"
        }
    },
    "BoundaryConditions":
    {
        "velocity":
        {
            "Dirichlet":
            {
                "inflow":
                {
                    // inflow condition
                    "expr":"{4*Um*y*( H-y )/H^2,0}:y:Um:H"
                },
                /* outflow condition */
                "wall":
                {
                    "markers":"mark",
                    "expr":"{0,0}"
                },
                "cylinder":
                {
                    "markers":["mark1","mark2"],
                    "expr":"{0,0}"
                }
            },
            "Neumann":
            {
                "outlet":
                {
                    "markers":
                    {
                        "name":"feelpp"
                    },
                    "expr":"{0,0,0,0}" // outlet
                }
            },
            "Robin":
            {
                "test":
                {
                    "markers":
                    {
                        "name":["mark%1%","mymarker","toto%2%","feelpp%1%_p%3%"],
                        "index1":"1:3",
                        "index2":"2:8:2",
                        "index3":["A","D"]
                    },
                    "material":"mycopper",
                    "expr1":"{1,2,3}",
                    "expr2":"{x,y,z}:x:y:z"
                }
            }
        }
    }, // BoundaryConditions
    "PostProcess":
    {
        "Force":["cylinder"],
        "PressureDifference":
        {
            "x1":"{0.15,0.2}",
            "x2":"{0.25,0.2}"
        },
        "Measures":
        {
            "Statistics":
            {
                "my_%1%_%2%_eval1":
                {
                    "type":"integrate",
                    "expr":"3.5*%2%",
                    "markers":
                    {
                        "name":"mat%1%%2%_%3%",
                        "index3":["x","y","z"]
                    },
                    "index1":["A","B"],
                    "index2":["3:9:2"]
                },
                "my_%1%_%2_2%_eval2":
                {
                    "type":"integrate",
                    "expr":"3.5*%2_1%",
                    "markers":"mat%1%%2_1%",
                    "index1":["A","B"],
                    "index2":[ ["3","trois"],["5","cinq"],["7","sept"] ]
                }
            }
        }
    },
    "Outputs":
    {
        "myoutput":
        {
            "type":"average",
            "markers":["marker1","marker2"],
            "topodim":3
        },
        "flux":
        {
            "type":"flux",
            "markers":"face1",
            "topodim":2
        }
    }
}

