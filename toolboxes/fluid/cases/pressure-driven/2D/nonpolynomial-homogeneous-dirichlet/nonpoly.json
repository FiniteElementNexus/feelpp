{
    "Name": "gravity",
    "ShortName":"gravity",
    "Models":
    {
        "equations":"Stokes"
    },
    "Parameters":
    {
        "pi":3.141592653589793,
        "nu":1,
        "fx":"(-nu*(-4*pi**3*sin(pi*x)**2*sin(pi*y)*cos(pi*y) - 4*pi**3*sin(pi*y)*cos(pi*x)**2*cos(pi*y)) + 8*pi**3*nu*sin(pi*x)**2*sin(pi*y)*cos(pi*y) - 8*pi**3*nu*sin(pi*y)*cos(pi*x)**2*cos(pi*y) - pi*sin(pi*x)*cos(pi*y)):x:y:nu:pi",
        "fy":"(-nu*(4*pi**3*sin(pi*x)*sin(pi*y)**2*cos(pi*x) + 4*pi**3*sin(pi*x)*cos(pi*x)*cos(pi*y)**2) - 8*pi**3*nu*sin(pi*x)*sin(pi*y)**2*cos(pi*x) + 8*pi**3*nu*sin(pi*x)*cos(pi*x)*cos(pi*y)**2 - pi*sin(pi*y)*cos(pi*x)):x:y:nu:pi",
        "uexx":"(2*pi*sin(pi*x)**2*sin(pi*y)*cos(pi*y)):x:y:pi",
        "uexy":"(-2*pi*sin(pi*x)*sin(pi*y)**2*cos(pi*x)):x:y:pi",
        "pex":"cos(pi*x)*cos(pi*y):x:y:pi",
        "ugradexxx":"4*pi**2*sin(pi*x)*sin(pi*y)*cos(pi*x)*cos(pi*y):x:y:pi",
        "ugradexxy":"2*pi**2*sin(pi*x)**2*sin(pi*y)**2 - 2*pi**2*sin(pi*y)**2*cos(pi*x)**2:x:y:pi",
        "ugradexyx":"-2*pi**2*sin(pi*x)**2*sin(pi*y)**2 + 2*pi**2*sin(pi*x)**2*cos(pi*y)**2:x:y:pi",
        "ugradexyy":"-4*pi**2*sin(pi*x)*sin(pi*y)*cos(pi*x)*cos(pi*y):x:y:pi"
    },
    "Materials":
    {
        "Omega":{
            "rho":"1",
            "mu":"nu:nu"
        }
    },
    "BoundaryConditions":
    {
        "velocity":
        {
            "VolumicForces":
            {
                "Omega":
                {
                    "expr":"{fx,fy}:fx:fy"
                }
            },
            "Dirichlet":
            {
                "top":
                {
                    "expr":"{0,0}"
                },
                "bottom":
                {
                    "expr":"{0,0}"
                }
            }
        },
        "pressure":
        {
            "Dirichlet":
            {
                "left":
                {
                    "expr":"pex:pex"
                },
                "right":
                {
                    "expr":"pex:pex"
                }
            }
        }
    },
	"Exact solution":
	{
	    "p_exact":
	    {
		    "Omega":
		    {
		        "expr":"pex:pex"
		    }
	    },
        "u_exact":
        {
            "Omega":
            {
                "expr":"{uexx,uexy}:uexx:uexy"
            }
        }
    },
    "PostProcess":
    {
        "Measures":
        {
            "Norm":
            {
                "p": 
                {
                    "type":"L2-error", 
                    "field":"pressure", 
                    "solution":"pex:pex",
                    "markers":"Omega" 
                },
                "u": 
                {
                    "type":["L2-error", "H1-error"], 
                    "field":"velocity", 
                    "solution":"{uexx,uexy}:uexx:uexy",
                    "grad_solution":"{ugradexxx,ugradexxy,ugradexyx,ugradexyy}:ugradexxx:ugradexxy:ugradexyx:ugradexyy",
                    "markers":"Omega" 
                }
            }
        },
        "Checkers":
        {
            "Measures":
            {
                "Norm_u_L2-error":
                {
                    "value":0,
                    "tolerance":1
                },
                "Norm_u_H1-error":
                {
                    "value":0,
                    "tolerance":1
                },
                "Norm_p_L2-error":
                {
                    "value":0,
                    "tolerance":1
                }
            }
        },
        "Exports":
        {
            "fields":["velocity","pressure","pid"],
            "expr":
            {
                "u_ex":"{uexx,uexy}:uexx:uexy",
                "p_ex":"pex:pex"
            }
        }
    }

}
