// -*- mode: javascript -*-
// Source for the values of rho, Cp, h : https://docs.lib.purdue.edu/iracc/1338/
{
    "Name": "Themal Fin {{ dim }}d",
    "ShortName":"ThermalFin{{ dim }}d",
    "CRBParameters":
    {
        {% for item in fins %}"k_{{ item }}":
        {
            "min":"0.1",
            "max":"10"
        },
        {% endfor %}"k_0":
        {
            "min":"1",
            "max":"1"
        },
        "Bi":
        {
            "min":"0.01",
            "max":"1"
        }
    },
    "Parameters":
    {
        {% for item in fins %}"k_{{ item }}": "0.1",
        {% endfor %}"k_0": "1",
        "Bi": "0.01"
    },
    "Materials":
    {
        {% for item in fins %}"Fin_{{ item }}":
        {
            "k":"k_{{ item }}:k_{{ item }}",
            "Cp":"1.4e6",
            "rho":"1"
        },
        {% endfor %}"Post":
        {
            "k":"k_0:k_0",
            "Cp":"1.4e6",
            "rho":"1"
        }
    },
    "InitialConditions":
    {
        "heat":
        {
            "temperature":
            {
                "Expression":
                {
                    "init":
                    {
                        "markers":[{% for item in fins %}"Fin_{{ item }}", {% endfor %}"Post"],
                        "expr":"0"
                    }
                }
            }
        }
    },
    "BoundaryConditions":
    {
        "heat":
        {
            "flux":
            {
                "Gamma_root":
                {
                    "expr":"1"
                }
            },
            "convective_heat_flux":
            {
                "Gamma_ext":
                {
                    "h":"Bi:Bi",
                    "Text":"0"
                }
            }
        }
    },
    "PostProcess":
    {
        "Exports":
        {
            "fields":["temperature","pid"]
        }
    }
}
