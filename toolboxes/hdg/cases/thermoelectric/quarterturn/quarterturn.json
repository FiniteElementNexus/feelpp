{
    "Name": "HDG-Electro-Thermo ",
    "ShortName":"ET",
    "Models":
    {
        "equations":"hdg",
        "p_expr":"2*dif/Pi*atan2(y,x):x:y:z:dif",
        "u_expr":"{2*sigma0*dif/Pi*y/(x*x+y*y),-2*sigma0*dif/Pi*x/(x*x+y*y),0}:x:y:z:dif:sigma0"
    },
    "Parameters":
    {
        "dif":0.03125,
        "intensity":-800,
        "sigma0":58e3,
        "k0":0.38,
        "h":0.08,
        "Tw":293
    },
    "Materials":
    {
        "omega":
        {
            "filename":"$cfgdir/omega.json"
        }
    },
    "BoundaryConditions":
    {
        "current-density":
        {
            "Integral":
            {
                "V0":
                {
                    "expr":"intensity:intensity"
                }
            }
        },
        "electric-potential":
        {
            "Dirichlet":
            {
                // "V0":
                // {
                //     "expr":"dif:dif"
                // },
                "V1":
                {
                    "expr":"0"
                }
            },
            "Neumann":
            {
                "Rint":
                {
                    "expr":"0"
                },
                "Rext":
                {
                    "expr":"0"
                },
                "top":
                {
                    "expr":"0"
                },
                "bottom":
                {
                    "expr":"0"
                }
            }
        },
        "temperature":
        {
            "Robin":
            {
                "Rint":
                {
                    "expr1":"-h:h",
                    "expr2":"-h*Tw:h:Tw"
                },
                "Rext":
                {
                    "expr1":"-h:h",
                    "expr2":"-h*Tw:h:Tw"
                }
            },
            "Neumann":
            {
                "V0":
                {
                    "expr":"0"
                },
                "V1":
                {
                    "expr":"0"
                },
                "top":
                {
                    "expr":"0"
                },
                "bottom":
                {
                    "expr":"0"
                }
            }
        }
    },
    "PostProcess":
    {
        "use-model-name":1,
        "electric":
        {
            "Measures":
            {
                "Points":
                {
                    "%1_1%":
                    {
                        "coord":"%1_2%",
                        "fields":"electric-potential",
                        "index1": [ ["V0", "{0,1.5,0.5}"],
                                    ["V1", "{1.5,0,0.5}"]
                                  ]
                    }
                },
                "Statistics":
                {
                    "Intentsity0":
                    {
                        "type":"integrate",
                        "field":"current-density",
                        "markers":"V0"
                    },
                    "Intentsity1":
                    {
                        "type":"integrate",
                        "field":"current-density",
                        "markers":"V1"
                    },
                    "Power":
                    {
                        "type":"integrate",
                        "expr":"-electric_Cx*electric_dxP-electric_Cy*electric_dyP-electric_Cz*electric_dzP:electric_Cx:electric_dxP:electric_Cy:electric_dyP:electric_Cz:electric_dzP",
                        "markers":"omega"
                    }
                }
            }
        },
        "heat":
        {
            "Measures":
            {
                "Points":
                {
                    "T0":
                    {
                        "coord":"{1.06,1.06,1}",
                        "fields":"temperature"
                    }
                },
                "Statistics":
                {
                    "MeantT":
                    {
                        "type":["min","max","mean"],
                        "field":"temperature"
                    },
		            "Flux_%1_1%":
		            {
			            "type":"integrate",
			            "expr":"h*(heat_T-Tw):heat_T:h:Tw",
			            "markers":"%1_1%",
			            "index1":[ ["Rext", "h", "Tw"],
				                   ["Rint", "h", "Tw"]
                                 ]
                    }
                }
            }
        }
    }
}
