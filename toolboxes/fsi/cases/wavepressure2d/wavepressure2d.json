// -*- mode: javascript -*-
{
    "Name": "fluid fsi",
    "ShortName":"Fluidfsi",
    "Models":
    {
        "fluid":{
            "materials":["Fluid"],
            "equations":"Navier-Stokes"
        },
        "solid":{
            "materials":["fsi-wall"],
            "setup":
            {
                "equations":"Generalised-String",
                "axi":{}
            }
        },
        "solid_1d":{
            "materials":["fsi-wall"]
        },
        "fsi":
        {
            //"materials":["Fluid","fsi-wall"],
            //"subphysics":["fluid","solid","solid_1d"],
            "setup":
            {
                "interface":"fsi-wall"
            }
        }
    },
    "Parameters":
    {
        "pIn":"2.0e4",
        "pTimeMax":"0.005"
    },
    "Materials":
    {
        "Fluid":{
            "rho":"1",
            "mu":"0.035"
        },
        "fsi-wall":{
            "E":"0.75e6",
            "nu":"0.5",
            "rho":"1.1"
        }
    },
    "Meshes":
    {
        "fluid":
        {
            "MeshMotion":
            {
                "ComputationalDomain":
                {
                    "markers":"Fluid",
                    "method":"harmonic_extension"
                },
                "Displacement":
                {
                    "Zero":["fluid-inlet"],
                    "Free":["fluid-outlet"]
                }
            }
        }
    },
    "BoundaryConditions":
    {
        "fluid":
        {
            "normal_stress":
            {
                "fluid-inlet":
                {
                    "expr": "-(pIn/2.)*(1-cos(pi*t/(pTimeMax/2.)))*(t<pTimeMax):pIn:pTimeMax:t"
                }
            },
            "outlet_windkessel":
            {
                "fluid-outlet":
                {
                    "coupling":"implicit", // explicit, implicit [default=implicit]
                    "Rd":6.2e3,            // resistance distal
                    "Rp":400,              // resistance proximal
                    "Cd":2.72e-4           // capacitance
                }
            }
        },
        "solid_1d":
        {
            "displacement_imposed":
            {
                "solid-fixed":
                {
                    "expr":"0"
                }
            }
        }
    },
    "PostProcess":
    {
        "fluid":{
            "Exports":
            {
                "fields":["velocity","pressure","mesh-displacement","pid"]
            }
        }
    }
}
