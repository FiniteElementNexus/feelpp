// -*- mode: javascript -*-
{
    "Name":"test2d",
    "ShortName":"test2d",
    "Models":
    {
        "heat":
        [
            {
                "name":"heat1",
                //"materials":["OmegaTE","OmegaHT"]
                "materials":["OmegaTE"]
            },
            {
                "name":"heat2",
                "materials":["OmegaHT"]
            }
        ],
        "electric":
        {
            "name":"elec1",
            "materials":["OmegaTE"]
        },
        "thermoelectric":
        {
            //"name":"bbb",
            //"materials":["OmegaTE"],
            "submodels":{ "heat":["heat1"], "electric":"elec1" }
        }
    },
    "Meshes":
    {
        "thermoelectric":
        {
            "Import":
            {
                "filename":"$cfgdir/test-thermoelectric.geo",
                "hsize":0.05
            }
        }
    },
    "Materials":
    {
        "OmegaTE":
        {
            //"physics":"thermo-electric",
            //"k":"370",//[ W/(m*K) ]
            "k":"100+350*(x*x+y*y):x:y",//[ W/(m*K) ]
            //"Cp":"1000.005", //[ J/(kg*K) ]
            // "rho":"1.205", //[ kg/(m^3) ]
            "sigma":"53e6"
            //"sigma":"53e6+y*y*1e8:y"
        },
        "OmegaHT":
        {
            //"physics":"heat",
            //"k":"1"
            "k":"10+350*(x*x+y*y):x:y"
        }
    },
    "BoundaryConditions":
    {
        "temperature":
        {
            "Dirichlet":
            {
                "outerHT":
                {
                    "expr":"0"
                }
            },
            "Neumann":
            {
                "insulation":
                {
                    "markers":["rightTE","leftTE","leftHT","rightHT","cylinder"],
                    "expr":"0" 
                }
            }
        },
        "electric-potential":
        {
            "Dirichlet":
            {
                "rightTE":
                {
                    "expr":"0.3"
                },
                "leftTE":
                {
                    "expr":"0.1"
                }
            }
        }
    },
    "PostProcess":
    {
        "thermoelectric":
        {
            "Exports":
            {
                "fields":["heat.temperature","electric.electric-potential","electric.electric-field","pid","all"]
            }
        }
    }

}
