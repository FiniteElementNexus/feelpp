// -*- mode: javascript -*-
{
    "Name":"test2d",
    "ShortName":"test2d",
    "Models":
    {
        "cfpdes":{
            "equations":["heat","electric"]
        },
        "heat":{
            "common":{
                "setup":{
                    "unknown":
                    {
                        "basis":"Pch1",
                        "name":"temperature",
                        "symbol":"T"
                    }
                }
            },
            "models":[
                {
                    "name":"heat_OmegaTE",
                    "materials":"OmegaTE",
                    "setup":{
                        "coefficients":{
                            "c":"materials_OmegaTE_k:materials_OmegaTE_k",
                            "f":"materials_OmegaTE_sigma*(electric_grad_P_0^2+electric_grad_P_1^2):materials_OmegaTE_sigma:electric_grad_P_0:electric_grad_P_1"
                        }
                    }
                },{
                    "name":"heat_OmegaHT",
                    "materials":"OmegaHT",
                    "setup":{
                        "coefficients":{
                            "c":"materials_OmegaHT_k:materials_OmegaHT_k"
                        }
                    }
                }
            ]
        },
        "electric":{
            "materials":"OmegaTE",
            "setup":{
                "unknown":
                {
                    "basis":"Pch1",
                    "name":"electric-potential",
                    "symbol":"P"
                },
                "coefficients":{
                    "c":"materials_OmegaTE_sigma:materials_OmegaTE_sigma"
                }
            }
        }
    },
    "Meshes":
    {
        "cfpdes":
        {
            "Import":
            {
                "filename":"$cfgdir/test-thermoelectric.geo",
                "hsize":0.05
            }
        }
    },
    "Materials":
    {
        "OmegaTE":
        {
            //"k":"370",//[ W/(m*K) ]
            "k":"100+350*(x*x+y*y):x:y",//[ W/(m*K) ]
            //"Cp":"1000.005", //[ J/(kg*K) ]
            // "rho":"1.205", //[ kg/(m^3) ]
            "sigma":"53e6"
            //"sigma":"53e6+y*y*1e8:y"
        },
        "OmegaHT":
        {
            //"k":"1"
            "k":"10+350*(x*x+y*y):x:y"
        }
    },
    "BoundaryConditions":
    {
        "heat":
        {
            "Dirichlet":
            {
                "outerHT":
                {
                    "expr":"0"
                }
            },
            "Neumann":
            {
                "insulation":
                {
                    "markers":["rightTE","leftTE","leftHT","rightHT","cylinder"],
                    "expr":"0" 
                }
            }
        },
        "electric":
        {
            "Dirichlet":
            {
                "rightTE":
                {
                    "expr":"0.3"
                },
                "leftTE":
                {
                    "expr":"0.1"
                }
            }
        }
    },
    "PostProcess":
    {
        "cfpdes":
        {
            "Exports":
            {
                "fields":["heat.temperature","electric.electric-potential","electric.electric-field","pid","all"]
            }
        },
        "heat":
        {
        },
        "electric":
        {
        }
    }

}
