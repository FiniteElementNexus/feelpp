// -*- mode: javascript -*-
{
    "Name": "Opus heat toolbox",
    "ShortName":"OpusHeat-tb",
    "Models":
    {
        "cfpdes":{
            "equations":"heat"
        },
        "heat":{
            "common":{
                "setup":{
                    "unknown":{
                        "basis":"Pch1",
                        "name":"temperature",
                        "symbol":"T"
                    }
                }
            },
            "models":[
                {
                    "name":"heat_PCB",
                    "materials":"PCB",
                    "setup":{
                        "coefficients":{
                            "c":"materials_PCB_k:materials_PCB_k"
                        }
                    }
                },{
                    "name":"heat_AIR",
                    "materials":"AIR",
                    "setup":{
                        "coefficients":{
                            "c":"materials_AIR_k:materials_AIR_k",
                            "beta":"{0,-materials_AIR_Cp*materials_AIR_rho*vinconv*(x-0.008)*(x-0.054)*(x>0.008)}:x:materials_AIR_Cp:materials_AIR_rho:vinconv"
                        }
                    }
                },{
                    "name":"heat_IC1",
                    "materials":"IC1",
                    "setup":{
                        "coefficients":{
                            "c":"materials_IC1_k:materials_IC1_k",
                            "f":"q_1:q_1"
                        }
                    }
                },{
                    "name":"heat_IC2",
                    "materials":"IC2",
                    "setup":{
                        "coefficients":{
                            "c":"materials_IC2_k:materials_IC2_k",
                            "f":"q_2:q_2"
                        }
                    }
                }
            ]
        }
    },
    "Parameters":
    {
        "k_1":
        {
            "value":"2",
            "min":"1",
            "max":"3"
        },
        "k_2":
        {
            "value":"2",
            "min":"1",
            "max":"3"
        },
        "hcoeff":
        {
            "value":"2",
            "min":"0.1",
            "max":"5"
        },
        "q_1":
        {
            "value":"1e5",
            "min":"1e4",
            "max":"1e6"
        },
        "q_2":
        {
            "value":"1e5",
            "min":"1e4",
            "max":"1e6"
        },
        "vinconv":
        {
            "value":"15",
            "min":"1",
            "max":"30"
        }
    },
    "Materials":
    {
        "PCB":
        {
            "k":"3e-2",
            "Cp":"2e6",
            "rho":"1"
        },
        "AIR":
        {
            "k":"3e-2",
            "Cp":"1",
            "rho":"1100"
        },
        "IC1":
        {
            "k":"k_1:k_1",
            "Cp":"1.4e6",
            "rho":"1"
        },
        "IC2":
        {
            "k":"k_2:k_2",
            "Cp":"1.4e6",
            "rho":"1"
        }
    },
    "BoundaryConditions":
    {
        "heat":
        {
            "Dirichlet":
            {
                "dirichlet":
                {
                    "markers":["Gamma_4_AIR1","Gamma_4_AIR4","Gamma_4_PCB"],
                    "expr":"300"
                }
            },
            "Robin":
            {
                "Gamma_2":
                {
                    "expr1":"hcoeff:hcoeff",// h coeff
                    "expr2":"300*hcoeff:hcoeff"// temperature exterior
                }
            }
        }
    },
    "PostProcess":
    {
        "cfpdes":
        {
            "Exports":
            {
                "fields":"all"//["temperature","pid"]
            }
        }
    }
}
