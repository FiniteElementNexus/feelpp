                {
                    "expr1":"h:h",
                    "expr2":"h*Tw:h:Tw",
                    "params":"h,Tw"
                },
                "R0n_R0":
                {
                    "expr1":"h:h",
                    "expr2":"h*Tw:h:Tw",
                    "params":"h,Tw"
                },
                "R1n_R0":
                {
                    "expr1":"h:h",
                    "expr2":"h*Tw:h:Tw",
                    "params":"h,Tw"
                },
                "Cooling_Slits_R0":
                {
                    "expr1":"h:h",
                    "expr2":"h*Tw:h:Tw",
                    "params":"h,Tw"
                },
                "BP_R0":
