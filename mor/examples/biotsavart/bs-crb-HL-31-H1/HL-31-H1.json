{
    "Name": "Thermoelectric",
    "ShortName": "Thermoelectric",
    "Model": "thermoelectric-crb",
    "Parameters":
    {
        "sigma":
        {
            "min":50e+3,
            "max":58e+3
        },
        "current":
        {
            "min":1,
            "max":10
        },
        "k":
        {
            "min":0.36,
            "max":0.39
        },
        "h":
        {
            "min":0.07,
            "max":0.1
        },
        "Tw":
        {
            "min":283,
            "max":303
        }
    },
    "BoundaryConditions":
    {
        "potential":
        {
            "Dirichlet":
            {
                "V1":
                {
                    "expr":"current:current",
                    "params":"current"
                },
                "V0":
                {
                    "expr":"0"
                }
            }
        },
        "temperature":
        {
            "Robin":
            {
                "Rext":
                {
                    "expr1":"h:h",
                    "expr2":"h*Tw:h:Tw",
                    "params":"h,Tw"
                },
                "Rint":
                {
                    "expr1":"h:h",
                    "expr2":"h*Tw:h:Tw",
                    "params":"h,Tw"
                }
            }
        }
    }
}
